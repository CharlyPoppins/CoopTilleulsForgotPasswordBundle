{
    "name": "coopTilleuls/forgot-password-bundle",
    "type": "symfony-bundle",
    "description": "Provides a 'forgot password' feature for a REST API",
    "keywords": [
        "Forgot password",
        "REST",
        "API"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Chalamon",
            "email": "vincentchalamon@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "sensio/framework-extra-bundle": "^2.3|^3.0",
        "symfony/http-foundation": "^2.3|^3.0",
        "symfony/http-kernel": "^2.3|^3.0",
        "symfony/security": "^2.3|^3.0",
        "symfony/event-dispatcher": "^2.3|^3.0",
        "symfony/dependency-injection": "^2.3|^3.0",
        "symfony/config": "^2.3|^3.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "stof/doctrine-extensions-bundle": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "<5.0",
        "symfony/browser-kit": "^2.3|^3.0",
        "symfony/security-bundle": "^2.3|^3.0",
        "symfony/swiftmailer-bundle": "^2.3|^3.0",
        "symfony/twig-bundle": "^2.3|^3.0",
        "symfony/assetic-bundle": "^2.8",
        "doctrine/data-fixtures": "^1.1",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "qandidate/symfony-json-request-transformer": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "ForgotPasswordBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ForgotPasswordBundle\\Tests\\": "features/app"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
